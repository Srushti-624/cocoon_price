{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1"
        }
    ],
    "framework": "vite",
    "env": {
        "PIP_BREAK_SYSTEM_PACKAGES": "1"
    }
}